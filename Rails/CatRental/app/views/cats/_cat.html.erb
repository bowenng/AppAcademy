<article>
    <h2>
        Name: <%= cat.name %>
    </h2>
    <p>
        Age: <%= cat.age %>
        Sex: <%= cat.sex %>
        Color: <%= cat.color %>
    </p>
    <p>
        Description:
        <%= cat.description %>
    </p>

    <p>
        <% if rent %>
            <%= rent.start_date %> -> <%= rent.end_date %>
        <% else %>
            Rent me!
        <% end %>
    </p>
</article>